<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/telefono"
        android:layout_width="226dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="112dp"
        android:layout_marginRight="112dp"
        android:ems="10"
        android:hint="@string/telefono"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/correo"
        android:layout_width="226dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="116dp"
        android:layout_marginRight="116dp"

        android:layout_marginBottom="16dp"
        android:ems="10"
        android:hint="@string/correo"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/paises"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/telefono"
        app:layout_constraintVertical_bias="0.176" />

    <AutoCompleteTextView
        android:id="@+id/paises"
        android:layout_width="229dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="100dp"


        android:hint="@string/pa_s"
        app:layout_constraintBottom_toTopOf="@+id/ciudad"
        tools:layout_editor_absoluteX="66dp" />

    <AutoCompleteTextView
        android:id="@+id/ciudad"
        android:layout_width="229dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:hint="@string/ciudad"
        app:layout_constraintBottom_toTopOf="@+id/siguiente2"
        tools:layout_editor_absoluteX="69dp" />

    <Button
        android:id="@+id/siguiente2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="76dp"
        android:layout_marginRight="76dp"
        android:text="@string/siguiente2"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="571dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="42dp"
        android:layout_height="50dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        app:layout_constraintEnd_toStartOf="@+id/telefono"
        app:srcCompat="@android:drawable/stat_sys_phone_call"
        tools:layout_editor_absoluteY="72dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="42dp"
        android:layout_height="53dp"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        app:layout_constraintEnd_toStartOf="@+id/correo"
        app:srcCompat="@android:drawable/ic_dialog_email"
        tools:layout_editor_absoluteY="192dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="37dp"
        android:layout_height="53dp"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        app:layout_constraintEnd_toStartOf="@+id/paises"
        app:srcCompat="@android:drawable/ic_menu_mapmode"
        tools:layout_editor_absoluteY="339dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="32dp"
        android:layout_height="50dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        app:layout_constraintEnd_toStartOf="@+id/ciudad"
        app:srcCompat="@android:drawable/ic_menu_gallery"
        tools:layout_editor_absoluteY="476dp" />


</androidx.constraintlayout.widget.ConstraintLayout>